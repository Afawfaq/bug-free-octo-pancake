# Enterprise Network Scan Configuration
# =====================================
# This configuration is designed for enterprise/corporate networks.
# Requires proper authorization before use!
#
# ⚠️ WARNING: Only use with explicit written permission from network owner.

# Enterprise subnet (adjust to your assigned testing range)
TARGET_NETWORK=10.0.0.0/24

# Core Infrastructure (customize for your target)
ROUTER_IP=10.0.0.1

# IoT Devices (set to actual IPs if known)
CHROMECAST_IP=10.0.0.50
TV_IP=10.0.0.51
PRINTER_IP=10.0.0.52
DLNA_IPS=10.0.0.53,10.0.0.54

# Scan Settings - More thorough for enterprise
PASSIVE_DURATION=120
DISCOVERY_RATE=2000
NUCLEI_SEVERITY=critical,high,medium,low

# Performance - Enable parallel for faster scanning
PARALLEL_EXECUTION=true
VERBOSE=true
SCAN_TIMEOUT=1800

# Enable comprehensive analysis
ENABLE_ATTACK_SURFACE=true
ENABLE_ADVANCED_MONITOR=true

# Output format
REPORT_FORMAT=all
